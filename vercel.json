{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "buildCommand": "npm run build",
  "installCommand": "rm -f package-lock.json && npm install",
  "redirects": [
    {
      "source": "https://www.acaboost.com/:path*",
      "destination": "https://acaboost.com/:path*",
      "permanent": true
    }
  ]
} 